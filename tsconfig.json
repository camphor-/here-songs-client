{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "types": ["@types/node", "@nuxt/vue-app", "@nuxtjs/axios"],
    "paths": { "@/*": ["./*"], "~/*": ["./*"] },
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "include": [
    "components/**/*.vue"
  ]
}
